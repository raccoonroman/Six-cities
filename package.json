    "@types/react-leaflet": "2.5.1",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "axios-mock-adapter": "1.17.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
